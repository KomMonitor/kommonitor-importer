{
  "realm" : "kommonitor",
  "resource" : "kommonitor-importer",
  "auth-server-url" : "https://kommonitor.muelheim-ruhr.de/auth",
  "ssl-required" : "external",
  "enable-cors" : true,
  "cors-max-age" : 1000,
  "cors-allowed-methods" : "POST, PUT, PATCH, DELETE, GET",
  "cors-exposed-headers" : "WWW-Authenticate",
  "bearer-only" : true,
  "enable-basic-auth" : false,
  "expose-token" : true,
  "verify-token-audience" : false,
   "credentials" : {
      "secret" : "secret"
   }
}